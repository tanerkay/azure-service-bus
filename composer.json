{
    "name": "tanerkay/azure-service-bus",
    "description": "This is a fork of now abandoned windows-azure for Service Bus.",
    "keywords": ["php", "azure", "sdk"],
    "license": "Apache-2.0",
    "type": "package",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tanerkay/laminas-captcha"
        },
        {
            "type": "vcs",
            "url": "https://github.com/tanerkay/laminas-mail"
        },
        {
            "type": "vcs",
            "url": "https://github.com/tanerkay/laminas-mime"
        },
        {
            "type": "vcs",
            "url": "https://github.com/tanerkay/laminas-recaptcha"
        },
        {
            "type": "vcs",
            "url": "https://github.com/tanerkay/laminas-json"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "pear/net_url2": "^2.2",
        "pear/mail_mime": "^1.10",
        "firebase/php-jwt": "^4.0 || ^5.0",
        "guzzlehttp/guzzle": "^7.0",
        "laminas/laminas-mime": "^2.6",
        "laminas/laminas-mail": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": { "AzureServiceBus\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    }
}